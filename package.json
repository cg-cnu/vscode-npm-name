{
    "name": "vscode-npm-name",
    "displayName": "NPM Name",
    "description": "Query and validate npm names",
    "version": "0.0.1",
    "publisher": "cg-cnu",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:npmName.query"
    ],
    "main": "./out/src/npmName",
    "contributes": {
        "commands": [
            {
                "command": "npmName.query",
                "title": "NPM Name"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "npm-name": "^3.1.0",
        "validate-npm-package-name": "^3.0.0"
    }
}
